import chromosome.ChromosomeCluster;
import chromosome.GeneticSlideImage;


public class TestShape {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String filename;
		ImageQue images=new ImageQue();
		filename=images.getNextFile(args[0]);
		GeneticSlideImage image=new GeneticSlideImage(filename);
		Extractor test2=new Extractor(image);						
		if(filename!=null){
			int shapeNum=test2.findBackground();
			shapeNum=test2.findChromosomes();
			ChromosomeCluster testCluster=test2.getClusterList().get(0);
			test2.getClusterList().get(0).clusterOut();
		}
	}

}
